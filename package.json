{
  "name": "@fleek-platform/next",
  "version": "1.0.0",
  "description": "Deploy your server-side Next.js application on Fleek",
  "bin": {
    "fleek-next": "./dist/index.js"
  },
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "module": "./dist/cli.js",
  "repository": "https://github.com/fleek-platform/fleek-next-cli",
  "homepage": "https://github.com/fleek-platform/fleek-next-cli/#readme",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nextjs",
    "next",
    "fleek",
    "server-side",
    "edge",
    "serverless"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier . --check",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@fleek-platform/functions-esbuild-config": "^0.0.6",
    "@fleek-platform/next-on-fleek": "1.14.4",
    "@fleek-platform/sdk": "2.1.5",
    "ansi-escapes": "^7.0.0",
    "as-table": "^1.0.55",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "esbuild": "^0.20.2",
    "esbuild-plugin-alias": "^0.2.1",
    "files-from-path": "^1.0.4",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.6.2",
    "server-only": "^0.0.1",
    "update-notifier": "^7.0.0",
    "update-notifier-cjs": "^5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/cli-progress": "^3.11.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
