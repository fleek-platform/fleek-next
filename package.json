{
  "name": "@fleekxyz/next",
  "version": "0.1.0",
  "description": "Deploy your server-side Next.js application on Fleek",
  "bin": {
    "fleek-next": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/fleekxyz/fleek-next-cli",
  "homepage": "https://github.com/fleekxyz/fleek-next-cli/#readme",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "next",
    "fleek",
    "server-side",
    "edge",
    "serverless"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "jest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier . --check",
      "eslint"
    ]
  },
  "dependencies": {
    "@fleekxyz/open-next": "3.0.1",
    "@fleekxyz/proxy": "0.0.8",
    "@fleekxyz/sdk": "0.7.3",
    "esbuild": "^0.20.2",
    "esbuild-plugin-alias": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
